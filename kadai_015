class Human:

    def __init__(self, name, age):
        self.name = name
        self.age = age

    def set_name(self, name, age):
        self.name = name
        self.age = age

    def printinfo(self):
        print("名前", self.name)
        print("年齢", self.age)

human = Human("TARO", 36)

human.printinfo()
